{
  "name": "manifast",
  "version-string": "0.1.0",
  "dependencies": [
    "fmt",
    "argparse",
    "gtest"
  ],
  "features": {
    "bundled-llvm": {
      "description": "Build and bundle LLVM from source via vcpkg",
      "dependencies": [
        {
          "name": "llvm",
          "features": [
            "target-all",
            "utils"
          ]
        }
      ]
    }
  },
  "default-features": [
    "bundled-llvm"
  ]
}