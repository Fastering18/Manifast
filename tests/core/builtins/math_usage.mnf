-- Test math native module
lokal math = impor("math")

assert(math.abs(-10) == 10, "abs gagal")
assert(math.floor(3.7) == 3, "floor gagal")
assert(math.ceil(3.2) == 4, "ceil gagal")
assert(math.sqrt(16) == 4, "sqrt gagal")

-- Trigo
lokal s = math.sin(math.pi / 2)
-- Note: pi is a constant in math module
assert(s > 0.99 dan s < 1.01, "sin pi/2 harus ~1")
println("s", s)
println("math.pi", math.pi)
println("math.e", math.e)